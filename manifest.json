{
    "name": "Cowductivity",
    "description": "A dead simple extension to help a certain lazy cow's productivity by blocking YouTube and Animes.",
    "version": "1.01",
    "manifest_version": 3,
    "action": {
        "default_popup": "ui.html",
        "default_icon": "cowicon.png"
    },
    "declarative_net_request": {
        "rule_resources": [{
            "id": "ruleset",
            "enabled": true,
            "path": "ruleset.json"
        }]
    },
    "permissions": ["storage", "declarativeNetRequest", "declarativeNetRequestFeedback"],
    "host_permissions": ["https://www.youtube.com/*"]
}